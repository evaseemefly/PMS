var myColumns = [[
                { field: 'ContentID', title: 'ID', width: 100,hidden:true },
                { field: 'UserName', title:'发送人', width: '8%'},
                { field: 'MissionName', title: '短信任务', width: '15%' },
                { field:'MessageType',title:'信息类型',width:'8%',
                    formatter: function (value) {
                        if (value==0) {
                            return '<span style="color:aquamarine;">' + '短信' + '</span>';
                        }
                        else {
                            return '<span style="color:gold;">' + '彩信' + '</span>';
                        }

                    }
                },
                { field: 'Content', title: '发送内容', width: '40%' },
                {
                    field: 'SendDateTime', title: '发送时间', width: '15%',
                    formatter: function (value) {
                        return Date.getStrOfDate(value);
                    }
                },
                { field: 'TotalOfReceiveNum', title: '发送总人数', width: '6%' },
                {
                    field: 'NotReceiveNum', title: '未收到人数', width: '6%',
                    formatter: function (value) {
                        if (value == 0) {
                            return '<span style="color:#4cff00;">' + value + '</span>';
                        }
                        else {
                            return '<span style="color:orangered;">' + value + '</span>';
                        }

                    }
                }
        ]]
