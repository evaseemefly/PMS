<div class="easyui-window" title="创建/修改调度作业" id="editWindow" style="width:700px;height:360px;overflow: hidden" data-options="shadow:true,modal:true">
    <iframe id="editFrame" frameborder="0" style="margin:0px 0px 0px 0px" width="100%" height="100%" @*scrolling="no"*@ @*不显示滚动条*@></iframe>
</div>
<script type="text/javascript">
    //设置编辑或创建窗体加载的远程url地址
    function showEditWindow(url) {
        //从指定页面中预读取数据
        $("#editFrame").attr("src", url);
        //打开编辑窗体
        $("#editWindow").window('open');
    }
</script>