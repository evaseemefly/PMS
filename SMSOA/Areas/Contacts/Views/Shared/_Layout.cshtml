</head>
@*overflow:hidden;*@
@*<body id="body" style="margin:0px;height:90%" >*@
    <body id="body" style="margin:0px;height:90%" onload="SetiframeHeight('body_layout')">
        @RenderBody()
        <div class="easyui-layout" id="body_layout" style="width:100%;height:91%;">
            @RenderSection("westdiv")
            <div data-options="region:'center'" title="短信联系人列表" style="width:70%">

                <table class="easyui-datagrid" id="persondatagrid" data-options="" style="width:100%;"></table>
                @RenderSection("person_tb", false)

            </div>
            @RenderSection("eastdiv",false)
        </div>
        @*群组、部门、任务均要重写*@
        @RenderSection("showWindow")

        @*只有部门、群组需要重写*@
        @RenderSection("editPersonWindow", false)
        @RenderSection("showMissionWindow", false)

        @*只有任务需要重写*@
        @RenderSection("showDepartmentWindow", false)
        @RenderSection("showGroupWindow", false)
    </body>
</html>



